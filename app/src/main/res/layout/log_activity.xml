<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:keepScreenOn="true"
    android:nestedScrollingEnabled="true"
    android:orientation="vertical"
    tools:content=".LogActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="84dp"
        android:layout_height="73dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="14dp"
        android:src="@drawable/logo" />

    <TextView
        android:id="@+id/my_health_c"
        android:layout_width="274dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="99dp"
        android:layout_marginTop="37dp"
        android:fontFamily="@font/lexend_deca"
        android:gravity="top"
        android:text="@string/LogName"
        android:textColor="#000000"
        android:textSize="16sp"
        android:typeface="normal" />

    <TextView
        android:id="@+id/some_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="175dp"
        android:layout_marginTop="168dp"
        android:fontFamily="@font/inter_bold"
        android:gravity="top"
        android:text="@string/log"
        android:textColor="#000000"
        android:textSize="24sp" />

    <EditText
        android:id="@+id/mailAndPhone"
        android:layout_width="282dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="63dp"
        android:layout_marginTop="237dp"
        android:background="@drawable/border"
        android:fontFamily="@font/inter_medium"
        android:gravity="top"
        android:nestedScrollingEnabled="false"
        android:paddingLeft="11dp"
        android:paddingTop="10dp"
        android:text="@string/mail"
        android:textAllCaps="false"
        android:textColor="@color/grey"
        android:textSize="20dp" />

    <EditText
        android:id="@+id/password"
        android:layout_width="282dp"
        android:layout_height="52dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_centerInParent="true"
        android:layout_marginLeft="65dp"
        android:layout_marginTop="327dp"
        android:background="@drawable/border"
        android:fontFamily="@font/inter_medium"
        android:gravity="top"
        android:nestedScrollingEnabled="false"
        android:paddingLeft="11dp"
        android:paddingTop="10dp"
        android:text="@string/password"
        android:textAllCaps="false"
        android:textColor="@color/grey"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/or"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="188dp"
        android:layout_marginTop="407dp"
        android:fontFamily="@font/inter_bold"
        android:gravity="top"
        android:text="@string/or"
        android:textColor="#000000"
        android:textSize="18sp" />

    <Button
        android:id="@+id/sberButton"
        android:layout_width="128dp"
        android:layout_height="59dp"
        android:layout_above="@+id/LogButton"
        android:layout_alignParentStart="true"
        android:layout_marginStart="64dp"
        android:layout_marginBottom="21dp"
        android:background="@drawable/border_sber" />

    <Button
        android:id="@+id/YandexButton"
        android:layout_width="128dp"
        android:layout_height="59dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="218dp"
        android:layout_marginTop="171dp"
        android:layout_marginBottom="233dp"
        android:background="@drawable/border_yandex" />

    <Button
        android:id="@+id/LogButton"
        android:layout_width="281dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/RegButton"

        android:layout_alignParentBottom="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="-1dp"
        android:layout_marginBottom="160dp"
        android:background="@drawable/border_log"
        android:elegantTextHeight="true"
        android:focusableInTouchMode="false"
        android:fontFamily="@font/inter_medium"
        android:hapticFeedbackEnabled="true"
        android:linksClickable="true"
        android:nestedScrollingEnabled="true"
        android:selectAllOnFocus="true"
        android:singleLine="true"
        android:text="@string/login"
        android:textColor="@color/white"
        />

    <Button
        android:id="@+id/RegButton"
        android:layout_width="280dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="66dp"
        android:layout_marginBottom="98dp"

        android:background="@drawable/border_reg"
        android:elegantTextHeight="true"
        android:focusableInTouchMode="false"
        android:fontFamily="@font/inter_medium"
        android:hapticFeedbackEnabled="true"
        android:linksClickable="true"
        android:nestedScrollingEnabled="true"
        android:selectAllOnFocus="true"
        android:singleLine="true"
        android:text="@string/registration"
        android:textColor="@color/black" />


</RelativeLayout>